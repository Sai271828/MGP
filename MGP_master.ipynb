    "\n",
    "\n",
    "        querydata = {'start': id*100, \"stop\":(id+1)*100}\n",
    "        endpoint = '/api/v2/MGP/acad/range'\n",
    "        data = json.loads(doquery(endpoint,token,querydata))\n",
    "        json_file = \"try.json\"\n",
    "\n",
    "        # Save data to a JSON file\n",
